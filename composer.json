{
    "name": "rayanlevert/object-mapper",
    "description": "Dependency-free object mapper from different sources of data for PHP",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rayan Levert",
            "email": "rayanlevert@msn.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-reflection": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "RayanLevert\\ObjectMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayanLevert\\ObjectMapper\\Tests\\": "tests/"
        }
    }
}
